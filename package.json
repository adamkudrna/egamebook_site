{
  "name": "egamebook_site",
  "version": "1.0.0",
  "description": "Egamebook official site",
  "devDependencies": {
    "@visionappscz/stylelint-config-visionapps-order": "^1.0.0",
    "autoprefixer": "^9.4.7",
    "clean-css-cli": "^4.2.1",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.5.3"
  },
  "scripts": {
    "lint:css": "stylelint --syntax scss \"_sass/main/**/*.scss\"",
    "build:css": "node-sass --output-style expanded --source-map true --source-map-contents true --output _css _sass/main/main.scss && postcss _css/main.css --replace && cleancss --level 1 --source-map --source-map-inline-sources --output _site/css/main.min.css _css/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filiph/egamebook_site.git"
  },
  "homepage": "https://egamebook.com",
  "engines": {
    "node": ">=8.12"
  }
}
